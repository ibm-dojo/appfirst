{
	"id": "simple2App1",
	"name": "simple2App1",
	"loaderConfig": {
		"paths": {
			"simple2App1": "dapp/samples"
		}
	},
	"modules": [
	],
	"controllers": [
		"dapp/controllers/delite/Init",
		"dapp/controllers/delite/Load",
		"dapp/controllers/delite/Transition",
		"dapp/controllers/delite/Logger"
	],

	"dependencies": [
		"deliteful/LinearLayout",
		"deliteful/ViewStack",
		"dapp/samples/simple2/viewController1",
		"dapp/samples/simple2/parentController1"
	],

	"containerSelector" : "#nested1App1dviewStack",

	"appLogging": {
		"logAll": 0,
		"logTimeStamp": 0,
		"loggingList": [
			"TestTransition"
